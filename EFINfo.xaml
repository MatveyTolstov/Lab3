<Window x:Class="Lab1Kokoro.EFINfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1Kokoro"
        mc:Ignorable="d"
        Title="EFINfo" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <DataGrid x:Name="Efful" AutoGenerateColumns="False" >
            <DataGrid.Columns>
                
                <DataGridTextColumn Binding="{Binding ID_TABLEBOOKING}" Header = "ID стола " />
                <DataGridTextColumn Binding="{Binding TABLE_NUMBER}" Header = "Номер стола" />
                <DataGridTextColumn Binding="{Binding BOOKING_DATE}" Header = "Дата брони" />
                <DataGridTextColumn Binding="{Binding CLIENTS.FIRSTNAME}" Header = "Клиенты" />
                <DataGridTextColumn Binding="{Binding EVENTS_CLIENTS.EVENT_NAME}" Header = "Названия событий" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
